{
    "modules": {
        "interaction": {
            "type": "MeshInteraction",
            "slots": {
                "window": "window"
            }
        },
        "window": {
            "type": "Window",
            "properties": {},
            "slots": {
                "brand-screen": null,
                "home-page": "home-page",
                "section-page": "section-page",
                "search-page": "search-page",
                "article-page": "article-page",
                "navigation": "navigation",
                "lightbox": "lightbox",
                "search": "top-bar-search"
            }
        },
        "navigation": {
            "type": "NavigationModule"
        },
        "app-banner": {
            "type": "AppBanner",
            "properties": {
                "min-fraction": 0.35,
                "max-fraction": 0.7
            }
        },
        "top-bar-search": {
            "type": "SearchBox"
        },
        "home-search": {
            "type": "SearchBox",
            "properties": {
                "width-request": 400,
                "halign": "center"
            }
        },
        "results-arrangement": {
            "type": "GridArrangement",
            "properties": {
                "bottom-buffer": 250
            }
        },
        "set-banner-card": {
            "type": "TextCard"
        },
        "set-banner-module": {
            "type": "SetBannerModule",
            "properties": {
                "halign": "center"
            },
            "slots": {
                "card-type": "set-banner-card"
            }
        },
        "item-group": {
            "type": "ItemGroupModule",
            "slots": {
                "arrangement": "results-arrangement",
                "card-type": "results-card"
            }
        },
        "section-page": {
            "type": "BannerTemplate",
            "properties": {
                "margin-start": 150,
                "margin-end": 150
            },
            "slots": {
                "banner": "set-banner-module",
                "content": "item-group"
            }
        },
        "home-page": {
            "type": "HomePageATemplate",
            "slots": {
                "top": "app-banner",
                "middle": "home-search",
                "bottom": "home-page-set-group",
                "basement": "home-page-basement-set-group"
            }
        },
        "results-card": {
            "type": "CardA"
        },
        "search-results": {
            "type": "SearchModule",
            "slots": {
                "arrangement": "results-arrangement",
                "card-type": "results-card",
                "article-suggestions": null,
                "category-suggestions": null
            },
            "properties": {
                "halign": "center",
                "message-justify": "left",
                "message-halign": "center"
            }
        },
        "home-card": {
            "type": "CardA"
        },
        "home-page-set-group": {
            "type": "SetGroupModule",
            "properties": {
                "expand": true,
                "halign": "center",
                "valign": "center",
                "max-children": 6
            },
            "slots": {
                "arrangement": {
                    "type": "OverflowArrangement",
                    "properties": {
                        "orientation": "horizontal"
                    }
                },
                "card-type": "home-card"
            }
        },
        "home-page-basement-set-group": {
            "type": "SetGroupModule",
            "properties": {
                "expand": true
            },
            "slots": {
                "arrangement": "results-arrangement",
                "card-type": "home-card"
            }
        },
        "results-search-banner": {
            "type": "SearchBannerModule",
            "properties": {
                "halign": "center"
            }
        },
        "search-page": {
            "type": "BannerTemplate",
            "properties": {
                "margin-start": 150,
                "margin-end": 150
            },
            "slots": {
                "banner": "results-search-banner",
                "content": "search-results"
            }
        },
        "lightbox-card": {
            "type": "MediaCard"
        },
        "lightbox": {
            "type": "LightboxModule",
            "slots": {
                "card-type": "lightbox-card"
            }
        },
        "document-card": {
            "type": "KnowledgeDocumentCard",
            "properties": {
                "show-top-title": true,
                "show-toc": true
            }
        },
        "article-page": {
            "type": "ArticleStackModule",
            "slots": {
                "card-type": "document-card"
            }
        }
    }
}
