{
    "modules": {
        "interaction": {
            "type": "MeshInteraction",
            "slots": {
                "window": "window"
            }
        },
        "window": {
            "type": "Window",
            "slots": {
                "search": {
                    "type": "SearchBox"
                },
                "navigation": {
                    "type": "Navigation"
                },
                "lightbox": {
                    "type": "MediaLightbox",
                    "slots": {
                        "card-type": {
                            "type": "MediaCard"
                        }
                    }
                },
                "home-page": "home-page",
                "section-page": "section-page",
                "search-page": "search-page",
                "article-page": "article-page"
            }
        },
        "section-page": {
            "type": "BannerTemplate",
            "properties": {
                "margin-start": 100,
                "margin-end": 100
            },
            "slots": {
                "banner": {
                    "type": "SetBanner",
                    "properties": {
                        "halign": "center"
                    },
                    "slots": {
                        "card-type": {
                            "type": "TitleCard"
                        }
                    }
                },
                "content": {
                    "type": "ItemGroup",
                    "slots": {
                        "arrangement": {
                            "type": "GridArrangement",
                            "properties": {
                                "bottom-buffer": 250
                            },
                            "slots": {
                                "card-type": {
                                    "type": "LegacyPolaroidCard"
                                }
                            }
                        }
                    }
                }
            }
        },
        "home-page": {
            "type": "HamburgerBasementTemplate",
            "slots": {
                "top": {
                    "type": "AppBanner",
                    "properties": {
                        "min-fraction": 0.1,
                        "max-fraction": 0.7
                    }
                },
                "middle": {
                    "type": "SearchBox",
                    "properties": {
                        "halign": "center"
                    }
                },
                "bottom": {
                    "type": "SetGroup",
                    "properties": {
                        "expand": true,
                        "halign": "center",
                        "valign": "center",
                        "max-children": 6
                    },
                    "slots": {
                        "arrangement": {
                            "type": "OverflowArrangement",
                            "properties": {
                                "orientation": "horizontal"
                            },
                            "slots": {
                                "card-type": {
                                    "type": "LegacyPolaroidCard"
                                }
                            }
                        }
                    }
                },
                "basement": {
                    "type": "SetGroup",
                    "properties": {
                        "expand": true
                    },
                    "slots": {
                        "arrangement": {
                            "type": "GridArrangement",
                            "properties": {
                                "bottom-buffer": 250
                            },
                            "slots": {
                                "card-type": {
                                    "type": "LegacyPolaroidCard"
                                }
                            }
                        }
                    }
                }
            }
        },
        "search-page": {
            "type": "BannerTemplate",
            "properties": {
                "margin-start": 100,
                "margin-end": 100
            },
            "slots": {
                "banner": {
                    "type": "SearchBanner",
                    "properties": {
                        "halign": "center"
                    }
                },
                "content": {
                    "type": "Search",
                    "slots": {
                        "arrangement": {
                            "type": "GridArrangement",
                            "properties": {
                                "bottom-buffer": 250
                            },
                            "slots": {
                                "card-type": {
                                    "type": "LegacyPolaroidCard"
                                }
                            }
                        }
                    },
                    "properties": {
                        "message-justify": "left",
                        "message-halign": "center"
                    }
                }
            }
        },
        "article-page": {
            "type": "ArticleStack",
            "slots": {
                "card-type": {
                    "type": "KnowledgeDocumentCard",
                    "properties": {
                        "show-toc": true
                    }
                }
            }
        }
    }
}
