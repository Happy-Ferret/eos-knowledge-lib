{
    "modules": {
        "interaction": {
            "type": "MeshInteraction",
            "slots": {
                "window": "window"
            }
        },
        "window": {
            "type": "Window",
            "properties": {},
            "slots": {
                "brand-screen": null,
                "home-page": "home-page",
                "section-page": "section-page",
                "search-page": "search-page",
                "article-page": "article-page",
                "navigation": "navigation",
                "lightbox": "lightbox",
                "search": "top-bar-search"
            }
        },
        "navigation": {
            "type": "NavigationModule"
        },
        "app-banner": {
            "type": "AppBanner",
            "properties": {
                "min-fraction": 0.4,
                "max-fraction": 0.7,
                "valign": "center",
                "halign": "center",
                "expand": false
            }
        },
        "home-page-set-group": {
            "type": "SetGroupModule",
            "properties": {
                "expand": true,
                "halign": "fill",
                "valign": "fill"
            },
            "slots": {
                "arrangement": {
                    "type": "TiledGridArrangement"
                },
                "card-type": "home-card"
            }
        },
        "home-page": {
            "type": "DividedBannerTemplate",
            "properties": {
                "row-spacing": 30,
                "row-homogeneous": true
            },
            "slots": {
                "top-left": "app-banner",
                "top-right": "home-search",
                "bottom": "home-page-set-group"
            }
        },
        "top-bar-search": {
            "type": "SearchBox"
        },
        "home-search": {
            "type": "SearchBox",
            "properties": {
                "width-request": 350,
                "visible": true,
                "shadow-type": "none",
                "halign": "center",
                "valign": "center"
            }
        },
        "home-card": {
            "type": "CardB"
        },
        "set-banner-card": {
            "type": "TextCard"
        },
        "set-banner-module": {
            "type": "SetBannerModule",
            "properties": {
                "valign": "end"
            },
            "slots": {
                "card-type": "set-banner-card"
            }
        },
        "item-group": {
            "type": "ItemGroupModule",
            "slots": {
                "arrangement": "results-arrangement",
                "card-type": "results-card"
            }
        },
        "section-page": {
            "type": "SidebarTemplate",
            "slots": {
                "content": "set-banner-module",
                "sidebar": "item-group"
            }
        },
        "results-arrangement": {
            "type": "ListArrangement"
        },
        "results-card": {
            "type": "TextCard"
        },
        "search-results": {
            "type": "SearchModule",
            "slots": {
                "arrangement": "results-arrangement",
                "card-type": "results-card",
                "article-suggestions": null,
                "category-suggestions": null
            },
            "properties": {
                "message-valign": "center"
            }
        },
        "results-search-banner": {
            "type": "SearchBannerModule",
            "properties": {
                "valign": "center"
            }
        },
        "search-page": {
            "type": "SidebarTemplate",
            "slots": {
                "content": "results-search-banner",
                "sidebar": "search-results"
            },
            "properties": {}
        },
        "lightbox-card": {
            "type": "MediaCard"
        },
        "lightbox": {
            "type": "LightboxModule",
            "slots": {
                "card-type": "lightbox-card"
            }
        },
        "document-card": {
            "type": "KnowledgeDocumentCard"
        },
        "article-page": {
            "type": "ArticleStackModule",
            "slots": {
                "card-type": "document-card"
            }
        }
    }
}
