{
    "modules": {
        "interaction": {
            "type": "BuffetInteraction",
            "properties": {
                "theme": "endless_buffet"
            },
            "slots": {
                "window": "window"
            }
        },
        "window": {
            "type": "Window",
            "properties": {
                "animations": false
            },
            "slots": {
                "search": {
                    "type": "SearchBox"
                },
                "lightbox": {
                    "type": "MediaLightbox",
                    "slots": {
                        "card-type": {
                            "type": "MediaCard"
                        }
                    }
                },
                "brand-page": {
                    "type": "AppBanner",
                    "properties": {
                        "max-fraction": 0.6,
                        "show-subtitle": true,
                        "subtitle-capitalization": "uppercase"
                    }
                },
                "home-page": "home-page",
                "section-page": "section-page",
                "search-page": "search-page",
                "all-sets-page": "all-sets-page",
                "article-page": "article-page"
            }
        },
        "all-sets-page": {
            "type": "SideMenuTemplate",
            "slots": {
                "context": "context-banner",
                "sidebar": "main-menu",
                "content": {
                    "type": "InfiniteScrollingLayout",
                    "slots": {
                        "content": {
                            "type": "ResponsiveMargins",
                            "slots": {
                                "content": {
                                    "type": "SetGroup",
                                    "slots": {
                                        "arrangement": {
                                            "type": "SquareGuysArrangement",
                                            "properties": {
                                                "spacing": 20
                                            },
                                            "slots": {
                                                "card-type": "set-card"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "section-page": {
            "type": "SideMenuTemplate",
            "slots": {
                "context": "context-banner",
                "sidebar": "main-menu",
                "content": {
                    "type": "InfiniteScrollingLayout",
                    "properties": {
                        "name": "theme-scroll"
                    },
                    "slots": {
                        "content": {
                            "type": "ResponsiveMargins",
                            "slots": {
                                "content": {
                                    "type": "TwoPieceTemplate",
                                    "properties": {
                                        "orientation": "vertical"
                                    },
                                    "slots": {
                                        "first": {
                                            "type": "Thematic",
                                            "properties": {
                                                "halign": "center",
                                                "margin-top": 20,
                                                "margin-bottom": 20,
                                                "margin-start": 20,
                                                "margin-end": 20,
                                                "scroll-server": "theme-scroll"
                                            },
                                            "slots": {
                                                "arrangement": {
                                                    "type": "HalfArrangement",
                                                    "properties": {
                                                        "spacing": 20
                                                    },
                                                    "slots": {
                                                        "card-type": "main-card"
                                                    }
                                                },
                                                "header-card-type": "header-card"
                                            }
                                        },
                                        "second": "category-suggestions"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "home-page": {
            "type": "SideMenuTemplate",
            "slots": {
                "context": "context-banner",
                "sidebar": "main-menu",
                "content": {
                    "type": "InfiniteScrollingLayout",
                    "slots": {
                        "content": {
                            "type": "ResponsiveMargins",
                            "slots": {
                                "content": {
                                    "type": "TwoPieceTemplate",
                                    "properties": {
                                        "orientation": "vertical"
                                    },
                                    "slots": {
                                        "first": {
                                            "type": "Highlights",
                                            "properties": {
                                                "halign": "center",
                                                "margin-top": 20,
                                                "margin-bottom": 20,
                                                "margin-start": 20,
                                                "margin-end": 20,
                                                "support-sets": 1
                                            },
                                            "slots": {
                                                "highlight-arrangement": {
                                                    "type": "WindshieldArrangement",
                                                    "properties": {
                                                        "spacing": 20
                                                    },
                                                    "slots": {
                                                        "card-type": {
                                                            "type": "PostCard",
                                                            "properties": {
                                                                "title-capitalization": "uppercase",
                                                                "context-capitalization": "uppercase"
                                                            }
                                                        },
                                                        "filter": "no-sets"
                                                    }
                                                },
                                                "support-card-type": {
                                                    "type": "SetPreviewCard",
                                                    "properties": {
                                                        "show-trigger": false
                                                    },
                                                    "slots": {
                                                        "arrangement": {
                                                            "type": "SquareGuysArrangement",
                                                            "properties": {
                                                                "spacing": 20,
                                                                "max-rows": 2
                                                            },
                                                            "slots": {
                                                                "card-type": "main-card",
                                                                "filter": "no-sets"
                                                            }
                                                        },
                                                        "header-card-type": "header-card"
                                                    }
                                                },
                                                "sets-filter": {
                                                    "type": "FeaturedFilter",
                                                    "properties": {
                                                        "invert": true
                                                    }
                                                }
                                            }
                                        },
                                        "second": "category-suggestions"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "search-page": {
            "type": "SideMenuTemplate",
            "slots": {
                "context": "context-banner",
                "sidebar": "main-menu",
                "content": {
                    "type": "Search",
                    "slots": {
                        "arrangement": {
                            "type": "ScrollingListArrangement",
                            "properties": {
                                "bottom-buffer": 250
                            },
                            "slots": {
                                "card-type": {
                                    "type": "ListCard"
                                }
                            }
                        },
                        "article-suggestions": "article-suggestions",
                        "category-suggestions": "category-suggestions"
                    },
                    "properties": {
                        "halign": "fill",
                        "message-halign": "center",
                        "message-justify": "left"
                    }
                }
            }
        },
        "article-page": {
            "type": "SideMenuTemplate",
            "slots": {
                "context": "context-banner",
                "sidebar": "main-menu",
                "content": {
                    "type": "ArticleStack",
                    "properties" : {
                        "do-sliding-animation": false,
                        "allow-navigation": "next"
                    },
                    "slots": {
                        "card-type": {
                            "type": "KnowledgeDocumentCard",
                            "properties": {
                                "show-titles": false,
                                "show-toc": false,
                                "custom-css": "reader2.css"
                            }
                        },
                        "nav-card-type": {
                            "type": "SequenceCard"
                        }
                    }
                }
            }
        },
        "article-suggestions": {
            "type": "SuggestedArticles",
            "slots": {
                "arrangement": {
                    "type": "SquareGuysArrangement",
                    "properties" : {
                        "max-rows": 1,
                        "spacing": 10,
                        "margin-start": 20,
                        "margin-end": 20,
                        "margin-bottom": 20
                    },
                    "slots": {
                        "card-type": "main-card"
                    }
                }
            }
        },
        "category-suggestions": {
            "type": "SuggestedCategories",
            "slots": {
                "arrangement": {
                    "type": "SquareGuysArrangement",
                    "properties": {
                        "max-rows": 1,
                        "valign": "end",
                        "spacing": 20,
                        "margin-start": 20,
                        "margin-end": 20,
                        "margin-bottom": 20
                    },
                    "slots": {
                        "card-type": "set-card"
                    }
                }
            }
        },
        "main-card": {
            "type": "ThumbCard",
            "properties": {
                "title-capitalization": "uppercase",
                "context-capitalization": "uppercase"
            }
        },
        "header-card": {
            "type": "TextCard",
            "properties": {
                "margin-top": 25
            }
        },
        "set-card": {
            "type": "PostCard",
            "properties": {
                "title-capitalization": "uppercase",
                "context-capitalization": "uppercase"
            }
        },
        "main-menu": {
            "type": "SuggestedCategories",
            "properties": {
                "show-title": false
            },
            "slots": {
                "arrangement": {
                    "type": "ScrollingListArrangement",
                    "slots": {
                        "card-type": {
                            "type": "TextCard",
                            "properties": {
                                "title-capitalization": "uppercase",
                                "margin-top": 5
                            }
                        },
                        "filter": {
                            "type": "FeaturedFilter"
                        }
                    }
                }
            }
        },
        "context-banner": {
            "type": "ContextBanner"
        },
        "no-sets": {
            "type": "SetsFilter",
            "properties": {
                "invert": true
            }
        }
    }
}
