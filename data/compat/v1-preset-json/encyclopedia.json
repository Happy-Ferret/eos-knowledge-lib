{
    "modules": {
        "interaction": {
            "type": "MeshInteraction",
            "slots": {
                "window": "window"
            }
        },
        "window": {
            "type": "EncyclopediaWindow",
            "properties": {},
            "slots": {
                "home-page": "home-page",
                "search-page": "search-page",
                "article-page": "article-page",
                "lightbox": "lightbox"
            }
        },
        "home-page": {
            "type": "EncyclopediaCoverTemplate",
            "slots": {
                "top": "app-banner",
                "bottom": "home-search-box"
            }
        },
        "search-page": {
            "type": "DividedBannerTemplate",
            "slots": {
                "top-left": "article-app-banner",
                "top-right": "article-search-box",
                "bottom": "search-results-paper-template"
            }
        },
        "search-results-paper-template": {
            "type": "PaperTemplate",
            "slots": {
                "content": "search-results-paper-content"
            }
        },
        "search-results-paper-content": {
            "type": "BannerTemplate",
            "properties": {
                "image-separator": true,
                "margin-start": 45,
                "margin-end": 45
            },
            "slots": {
                "banner": "results-search-banner",
                "content": "search-results"
            }
        },
        "article-page": {
            "type": "DividedBannerTemplate",
            "slots": {
                "top-left": "article-app-banner",
                "top-right": "article-search-box",
                "bottom": "article-paper-template"
            }
        },
        "article-paper-template": {
            "type": "PaperTemplate",
            "slots": {
                "content": "article-stack"
            }
        },
        "article-stack": {
            "type": "ArticleStackModule",
            "slots": {
                "card-type": "document-card"
            }
        },
        "app-banner": {
            "type": "AppBanner",
            "properties": {
                "min-fraction": 0.5,
                "max-fraction": 0.5,
                "margin-bottom": 42
            }
        },
        "article-search-box": {
            "type": "SearchBox",
            "properties": {
                "halign": "center",
                "hexpand": true
            }
        },
        "home-search-box": {
            "type": "SearchBox",
            "properties": {
                "max-width-chars": 52
            }
        },
        "article-app-banner": {
            "type": "AppBanner",
            "properties": {
                "min-fraction": 0.2,
                "max-fraction": 0.2,
                "margin-top": 10,
                "margin-bottom": 10,
                "valign": "start",
                "vexpand": false,
                "halign": "center"
            }
        },
        "lightbox-card": {
            "type": "MediaCard"
        },
        "lightbox": {
            "type": "LightboxModule",
            "slots": {
                "card-type": "lightbox-card"
            }
        },
        "document-card": {
            "type": "KnowledgeDocumentCard"
        },
        "results-search-banner": {
            "type": "SearchBannerModule",
            "properties": {
                "halign": "start"
            }
        },
        "search-results": {
            "type": "SearchModule",
            "properties": {
                "margin-top": 20,
                "message-halign": "start",
                "halign": "start"
            },
            "slots": {
                "arrangement": {
                    "type": "ListArrangement",
                    "properties": {
                        "hexpand": true,
                        "halign": "fill"
                    }
                },
                "card-type": "results-card",
                "article-suggestions": null,
                "category-suggestions": null
            }
        },
        "results-card": {
            "type": "TextCard",
            "properties": {
                "underline-on-hover": true,
                "decoration": true
            }
        }
    }
}
