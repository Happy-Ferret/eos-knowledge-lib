{
    "modules": {
        "interaction": {
            "type": "BuffetInteraction",
            "properties": {
                "theme": "news"
            },
            "slots": {
                "window": "window"
            }
        },
        "window": {
            "type": "Window",
            "slots": {
                "search": {
                    "type": "SearchBox"
                },
                "navigation": null,
                "lightbox": {
                    "type": "LightboxModule",
                    "slots": {
                        "card-type": {
                            "type": "MediaCard"
                        }
                    }
                },
                "brand-page": {
                    "type": "AppBanner",
                    "properties": {
                        "max-fraction": 0.6,
                        "show-subtitle": true
                    }
                },
                "home-page": "home-page",
                "section-page": "section-page",
                "search-page": "search-page",
                "all-sets-page": "all-sets-page",
                "article-page": "article-page"
            }
        },
        "home-page": {
            "type": "TopMenuTemplate",
            "slots": {
                "top-menu": "main-menu",
                "content": {
                    "type": "ScrollingTemplate",
                    "slots": {
                        "content": {
                            "type": "BackgroundModule",
                            "properties": {
                                "page-mode": "home"
                            },
                            "slots": {
                                "content": {
                                    "type": "ResponsiveMarginsModule",
                                    "slots": {
                                        "content": {
                                            "type": "HighlightsModule",
                                            "properties": {
                                                "halign": "center",
                                                "margin-top": 50,
                                                "margin-bottom": 50,
                                                "support-sets": 4
                                            },
                                            "slots": {
                                                "highlight-arrangement": {
                                                    "type": "QuiltArrangement",
                                                    "properties": {
                                                        "spacing": 20
                                                    },
                                                    "slots": {
                                                        "card-type": {
                                                            "type": "PostCard",
                                                            "properties": {
                                                                "context-capitalization": "lowercase",
                                                                "text-halign": "start"
                                                            }
                                                        },
                                                        "order": {
                                                            "type": "PublishedDateOrder",
                                                            "properties": {
                                                                "ascending": false
                                                            }
                                                        }
                                                    }
                                                },
                                                "support-card-type": "set-preview",
                                                "sets-filter": {
                                                    "type": "FeaturedFilter"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "section-page": {
            "type": "TopMenuTemplate",
            "slots": {
                "top-menu": "main-menu",
                "content": {
                    "type": "ScrollingTemplate",
                    "properties": {
                        "name": "theme-scroll",
                        "bottom-buffer": 250
                    },
                    "slots": {
                        "content": {
                            "type": "BackgroundModule",
                            "properties": {
                                "page-mode": "section"
                            },
                            "slots": {
                                "content": {
                                    "type": "BannerTemplate",
                                    "properties": {
                                        "image-separator": true
                                    },
                                    "slots": {
                                        "banner": {
                                            "type": "SetBannerModule",
                                            "properties": {
                                                "halign": "center",
                                                "margin-top": 40
                                            },
                                            "slots": {
                                                "card-type": {
                                                    "type": "TextCard",
                                                    "properties": {
                                                        "title-capitalization": "lowercase"
                                                    }
                                                }
                                            }
                                        },
                                        "content": {
                                            "type": "TwoPieceTemplate",
                                            "properties": {
                                                "orientation": "vertical"
                                            },
                                            "slots": {
                                                "first": {
                                                    "type": "ResponsiveMarginsModule",
                                                    "slots": {
                                                        "content": {
                                                            "type": "HierarchicalSetModule",
                                                            "properties": {
                                                                "halign": "center",
                                                                "margin-top": 20,
                                                                "margin-bottom": 20,
                                                                "margin-start": 20,
                                                                "margin-end": 20,
                                                                "scroll-server": "theme-scroll"
                                                            },
                                                            "slots": {
                                                                "arrangement": {
                                                                    "type": "ListArrangement",
                                                                    "properties": {
                                                                        "vscrollbar-policy": "never",
                                                                        "spacing": 20
                                                                    },
                                                                    "slots": {
                                                                        "card-type": {
                                                                            "type": "SearchResultCard"
                                                                        }
                                                                    }
                                                                },
                                                                "set-card-type": "set-preview"
                                                            }
                                                        }
                                                    }
                                                },
                                                "second": "article-suggestions"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "search-page": {
            "type": "TopMenuTemplate",
            "slots": {
                "top-menu": "main-menu",
                "content": {
                    "type": "BackgroundModule",
                    "properties": {
                        "page-mode": "search"
                    },
                    "slots": {
                        "content": {
                            "type": "BannerTemplate",
                            "slots": {
                                "banner": {
                                    "type": "SearchBannerModule"
                                },
                                "content": {
                                    "type": "SearchModule",
                                    "slots": {
                                        "arrangement": {
                                            "type": "ListArrangement",
                                            "properties": {
                                                "bottom-buffer": 250
                                            },
                                            "slots": {
                                                "card-type": {
                                                    "type": "SearchResultCard"
                                                }
                                            }
                                        },
                                        "article-suggestions": {
                                            "type": "ResponsiveMarginsModule",
                                            "slots": {
                                                "content": {
                                                    "type": "SuggestedArticlesModule",
                                                    "properties": {
                                                        "show-trigger": false,
                                                        "title-capitalization": "lowercase",
                                                        "margin-top": 40,
                                                        "margin-bottom": 40,
                                                        "valign": "end"
                                                    },
                                                    "slots": {
                                                        "arrangement": {
                                                            "type": "SquareGuysArrangement",
                                                            "properties" : {
                                                                "max-rows": 1,
                                                                "spacing": 20,
                                                                "margin-bottom": 20
                                                            },
                                                            "slots": {
                                                                "card-type": {
                                                                    "type": "ThumbCard",
                                                                    "properties": {
                                                                        "context-capitalization": "lowercase",
                                                                        "text-halign": "start"
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    },
                                    "properties": {
                                        "halign": "fill",
                                        "message-justify": "left"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "all-sets-page": {
            "type": "TopMenuTemplate",
            "slots": {
                "top-menu": "main-menu",
                "content": {
                    "type": "ScrollingTemplate",
                    "slots": {
                        "content": {
                            "type": "ResponsiveMarginsModule",
                            "slots": {
                                "content": {
                                    "type": "SetGroupModule",
                                    "slots": {
                                        "arrangement": {
                                            "type": "ThirdRockArrangement",
                                            "properties": {
                                                "spacing": 20
                                            },
                                            "slots": {
                                                "card-type": "set-card"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "article-page": {
            "type": "TopMenuTemplate",
            "slots": {
                "top-menu": "main-menu",
                "content": {
                    "type": "BackgroundModule",
                    "properties": {
                        "page-mode": "article"
                    },
                    "slots": {
                        "content": {
                            "type": "ArticleStackModule",
                            "properties" : {
                                "do-sliding-animation": false,
                                "allow-navigation": "next"
                            },
                            "slots": {
                                "card-type": {
                                    "type": "KnowledgeDocumentCard",
                                    "properties": {
                                        "show-top-title": false,
                                        "show-toc": false
                                    }
                                },
                                "nav-card-type": {
                                    "type": "SearchResultCard",
                                    "properties": {
                                        "show-synopsis": false
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "article-suggestions": {
            "type": "ResponsiveMarginsModule",
            "slots": {
                "content": {
                    "type": "SupplementaryArticlesModule",
                    "properties": {
                        "same-set": false,
                        "show-trigger": false,
                        "title-capitalization": "lowercase",
                        "margin-top": 40,
                        "margin-bottom": 40,
                        "valign": "end"
                    },
                    "slots": {
                        "arrangement": {
                            "type": "SquareGuysArrangement",
                            "properties" : {
                                "max-rows": 1,
                                "spacing": 10,
                                "margin-start": 20,
                                "margin-end": 20,
                                "margin-bottom": 20
                            },
                            "slots": {
                                "card-type": {
                                    "type": "ThumbCard",
                                    "properties": {
                                        "context-capitalization": "lowercase",
                                        "text-halign": "start"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "set-slices": {
            "type": "SuggestedCategoriesModule",
            "slots": {
                "arrangement": {
                    "type": "SquareGuysArrangement",
                    "properties": {
                        "max-rows": 1,
                        "valign": "end",
                        "spacing": 20,
                        "margin-start": 20,
                        "margin-end": 20,
                        "margin-bottom": 20
                    },
                    "slots": {
                        "card-type": "set-card"
                    }
                }
            }
        },
        "main-card": {
            "type": "PostCard",
            "properties": {
                "context-capitalization": "lowercase",
                "text-halign": "start"
            }
        },
        "header-card": {
            "type": "TextCard",
            "properties": {
                "margin-top": 25
            }
        },
        "set-card": {
            "type": "TextCard",
            "properties": {
                "title-capitalization": "uppercase"
            }
        },
        "set-preview": {
            "type": "SetPreviewCard",
            "slots": {
                "arrangement": {
                    "type": "ThirtiesArrangement",
                    "properties": {
                        "spacing": 20,
                        "max-rows": 1
                    },
                    "slots": {
                        "card-type": "main-card",
                        "order": {
                            "type": "PublishedDateOrder",
                            "properties": {
                                "ascending": false
                            }
                        }
                    }
                },
                "header-card-type": "header-card"
            }
        },
        "main-menu": {
            "type": "ResponsiveMarginsModule",
            "slots": {
                "content": {
                    "type": "TopMenuModule",
                    "slots": {
                        "banner": {
                            "type": "AppBanner",
                            "properties": {
                                "max-fraction": 0.15,
                                "show-subtitle": false,
                                "visible": true
                            }
                        },
                        "menu": {
                            "type": "SuggestedCategoriesModule",
                            "properties": {
                                "show-title": false,
                                "visible": true
                            },
                            "slots": {
                                "arrangement": {
                                    "type": "SideBySideArrangement",
                                    "properties": {
                                        "halign": "center",
                                        "visible": true
                                    },
                                    "slots": {
                                        "card-type": {
                                            "type": "TextCard",
                                            "properties": {
                                                "title-capitalization": "lowercase",
                                                "margin-top": 5,
                                                "halign": "center",
                                                "decorate-on-highlight": true
                                            }
                                        },
                                        "filter": {
                                            "type": "FeaturedFilter"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
