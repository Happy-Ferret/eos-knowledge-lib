{
    "modules": {
        "interaction": {
            "type": "BuffetInteraction",
            "properties": {
                "theme": "news"
            },
            "slots": {
                "window": "window"
            }
        },
        "window": {
            "type": "Window",
            "properties": {
                "animations": false
            },
            "slots": {
                "search": {
                    "type": "SearchBox"
                },
                "navigation": null,
                "lightbox": {
                    "type": "MediaLightbox",
                    "slots": {
                        "card-type": {
                            "type": "MediaCard"
                        }
                    }
                },
                "brand-page": {
                    "type": "AppBanner",
                    "properties": {
                        "max-fraction": 0.6,
                        "show-subtitle": true
                    }
                },
                "home-page": "home-page",
                "section-page": "section-page",
                "search-page": "search-page",
                "article-page": "article-page"
            }
        },
        "home-page": {
            "type": "TopMenuTemplate",
            "slots": {
                "top-menu": "main-menu",
                "content": {
                    "type": "InfiniteScrollingLayout",
                    "slots": {
                        "content": {
                            "type": "Background",
                            "properties": {
                                "page-mode": "home"
                            },
                            "slots": {
                                "content": {
                                    "type": "ResponsiveMargins",
                                    "slots": {
                                        "content": {
                                            "type": "Highlights",
                                            "properties": {
                                                "halign": "center",
                                                "margin-top": 50,
                                                "margin-bottom": 50,
                                                "margin-start": 20,
                                                "margin-end": 20,
                                                "support-sets": 4
                                            },
                                            "slots": {
                                                "highlight-arrangement": {
                                                    "type": "PianoArrangement",
                                                    "properties": {
                                                        "spacing": 20
                                                    },
                                                    "slots": {
                                                        "card-type": {
                                                            "type": "PostCard",
                                                            "properties": {
                                                                "context-capitalization": "lowercase",
                                                                "text-halign": "start"
                                                            }
                                                        },
                                                        "filter": "no-sets",
                                                        "order": {
                                                            "type": "PublishedDateOrder",
                                                            "properties": {
                                                                "ascending": false
                                                            }
                                                        }
                                                    }
                                                },
                                                "support-card-type": "set-preview",
                                                "sets-filter": {
                                                    "type": "FeaturedFilter"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "section-page": {
            "type": "TopMenuTemplate",
            "slots": {
                "top-menu": "main-menu",
                "content": {
                    "type": "InfiniteScrollingLayout",
                    "id": "theme-scroll",
                    "properties": {
                        "bottom-buffer": 250
                    },
                    "slots": {
                        "content": {
                            "type": "Background",
                            "properties": {
                                "page-mode": "section"
                            },
                            "slots": {
                                "content": {
                                    "type": "BannerTemplate",
                                    "slots": {
                                        "banner": {
                                            "type": "SetBanner",
                                            "properties": {
                                                "halign": "center",
                                                "margin-top": 40
                                            },
                                            "slots": {
                                                "card-type": {
                                                    "type": "TitleCard",
                                                    "properties": {
                                                        "title-capitalization": "lowercase"
                                                    }
                                                }
                                            }
                                        },
                                        "content": {
                                            "type": "TwoPieceTemplate",
                                            "properties": {
                                                "orientation": "vertical"
                                            },
                                            "slots": {
                                                "first": {
                                                    "type": "ResponsiveMargins",
                                                    "slots": {
                                                        "content": {
                                                            "type": "HierarchicalSet",
                                                            "properties": {
                                                                "halign": "center",
                                                                "margin-top": 20,
                                                                "margin-bottom": 20,
                                                                "margin-start": 20,
                                                                "margin-end": 20
                                                            },
                                                            "slots": {
                                                                "arrangement": {
                                                                    "type": "ListArrangement",
                                                                    "properties": {
                                                                        "row-spacing": 20,
                                                                        "orientation": "vertical"
                                                                    },
                                                                    "slots": {
                                                                        "card-type": {
                                                                            "type": "ListCard"
                                                                        },
                                                                       "order": {
                                                                            "type": "PublishedDateOrder",
                                                                            "properties": {
                                                                                "ascending": false
                                                                            }
                                                                        }
                                                                    }
                                                                },
                                                                "set-card-type": "set-preview"
                                                            },
                                                            "references": {
                                                                "scroll-server": "theme-scroll"
                                                            }
                                                        }
                                                    }
                                                },
                                                "second": "article-suggestions"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "search-page": {
            "type": "TopMenuTemplate",
            "slots": {
                "top-menu": "main-menu",
                "content": {
                    "type": "Background",
                    "properties": {
                        "page-mode": "search"
                    },
                    "slots": {
                        "content": {
                            "type": "BannerTemplate",
                            "slots": {
                                "banner": {
                                    "type": "SearchBanner",
                                    "properties": {
                                        "halign": "center",
                                        "xalign": 0.5,
                                        "justify": "center"
                                    }
                                },
                                "content": {
                                    "type": "Search",
                                    "slots": {
                                        "arrangement": {
                                            "type": "ScrollingListArrangement",
                                            "properties": {
                                                "bottom-buffer": 250
                                            },
                                            "slots": {
                                                "card-type": {
                                                    "type": "ListCard",
                                                    "properties": {
                                                        "margin-start": 200,
                                                        "margin-end": 200
                                                    }
                                                }
                                            }
                                        },
                                        "article-suggestions": {
                                            "type": "ResponsiveMargins",
                                            "slots": {
                                                "content": {
                                                    "type": "SuggestedArticles",
                                                    "properties": {
                                                        "show-trigger": false,
                                                        "title-capitalization": "lowercase",
                                                        "margin-top": 50,
                                                        "margin-bottom": 50,
                                                        "valign": "end"
                                                    },
                                                    "slots": {
                                                        "arrangement": {
                                                            "type": "SquareGuysArrangement",
                                                            "properties" : {
                                                                "max-rows": 1,
                                                                "spacing": 20,
                                                                "margin-bottom": 20
                                                            },
                                                            "slots": {
                                                                "card-type": {
                                                                    "type": "ThumbCard",
                                                                    "properties": {
                                                                        "context-capitalization": "lowercase",
                                                                        "text-halign": "start"
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    },
                                    "properties": {
                                        "halign": "fill",
                                        "message-halign": "start",
                                        "message-justify": "left"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "article-page": {
            "type": "TopMenuTemplate",
            "slots": {
                "top-menu": "main-menu",
                "content": {
                    "type": "ArticleStack",
                    "properties" : {
                        "do-sliding-animation": false,
                        "allow-navigation": "next"
                    },
                    "slots": {
                        "card-type": {
                            "type": "KnowledgeDocumentCard",
                            "properties": {
                                "show-titles": false,
                                "show-toc": false
                            }
                        },
                        "nav-card-type": {
                            "type": "ListCard",
                            "properties": {
                                "show-synopsis": false
                            }
                        }
                    }
                }
            }
        },
        "article-suggestions": {
            "type": "ResponsiveMargins",
            "slots": {
                "content": {
                    "type": "SupplementaryArticles",
                    "properties": {
                        "same-set": false,
                        "show-trigger": false,
                        "title-capitalization": "lowercase",
                        "margin-start": 20,
                        "margin-top": 50,
                        "margin-bottom": 50,
                        "valign": "end"
                    },
                    "slots": {
                        "arrangement": {
                            "type": "SquareGuysArrangement",
                            "properties" : {
                                "max-rows": 1,
                                "spacing": 20,
                                "margin-end": 20,
                                "margin-bottom": 20
                            },
                            "slots": {
                                "card-type": {
                                    "type": "ThumbCard",
                                    "properties": {
                                        "context-capitalization": "lowercase",
                                        "text-halign": "start"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "main-card": {
            "type": "PostCard",
            "properties": {
                "context-capitalization": "lowercase",
                "text-halign": "start"
            }
        },
        "header-card": {
            "type": "TitleCard",
            "properties": {
                "margin-top": 25
            }
        },
        "set-card": {
            "type": "TitleCard",
            "properties": {
                "title-capitalization": "uppercase"
            }
        },
        "set-preview": {
            "type": "SetPreviewCard",
            "properties": {
                "trigger-capitalization": "lowercase"
            },
            "slots": {
                "arrangement": {
                    "type": "ThirtiesArrangement",
                    "properties": {
                        "spacing": 20,
                        "max-rows": 1
                    },
                    "slots": {
                        "card-type": "main-card",
                        "filter": "no-sets",
                        "order": {
                            "type": "PublishedDateOrder",
                            "properties": {
                                "ascending": false
                            }
                        }
                    }
                },
                "header-card-type": "header-card"
            }
        },
        "main-menu": {
            "type": "ResponsiveMargins",
            "slots": {
                "content": {
                    "type": "TopMenu",
                    "slots": {
                        "banner": {
                            "type": "AppBanner",
                            "properties": {
                                "max-fraction": 0.2,
                                "show-subtitle": false,
                                "visible": true
                            }
                        },
                        "menu": {
                            "type": "SuggestedCategories",
                            "properties": {
                                "show-title": false,
                                "visible": true
                            },
                            "slots": {
                                "arrangement": {
                                    "type": "SideBySideArrangement",
                                    "properties": {
                                        "halign": "center",
                                        "visible": true
                                    },
                                    "slots": {
                                        "card-type": {
                                            "type": "TitleCard",
                                            "properties": {
                                                "title-capitalization": "lowercase",
                                                "halign": "center",
                                                "decorate-on-highlight": true
                                            }
                                        },
                                        "filter": {
                                            "type": "FeaturedFilter"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "no-sets": {
            "type": "SetsFilter",
            "properties": {
                "invert": true
            }
        }
    }
}
