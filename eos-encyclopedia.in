#!/bin/bash
# For legacy purposes only. New encyclopedia apps ship bin script in the bundle
# and call ekn-app-runner.

if [ $# -lt 1 ]; then
    echo "This script if for legacy encyclopedia bundles only, and needs an app id."
    exit 1
fi

APP_ID="$1"
shift
ekn-app-runner $APP_ID %compatdatadir%/${APP_ID}.gresource $@
