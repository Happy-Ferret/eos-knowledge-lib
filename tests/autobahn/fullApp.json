{
  "version": 2,
  "root": {
    "type": "MeshInteraction",
    "slots": {
      "window": {
        "type": "EncyclopediaWindow",
        "slots": {
          "article-page": {
            "type": "DividedBannerTemplate",
            "slots": {
              "bottom": {
                "type": "PaperTemplate",
                "slots": {
                  "content": {
                    "type": "ArticleStack",
                    "properties": {
                      "do-sliding-animation": false
                    },
                    "slots": {
                      "card": {
                        "type": "KnowledgeDocumentCard"
                      }
                    }
                  }
                }
              },
              "top-left": {
                "type": "AppBanner",
                "properties": {
                  "halign": "center",
                  "margin-bottom": 10,
                  "margin-top": 10,
                  "valign": "start",
                  "vexpand": false
                }
              },
              "top-right": {
                "type": "SearchBox",
                "properties": {
                  "halign": "center",
                  "hexpand": true,
                  "valign": "center"
                }
              }
            }
          },
          "home-page": {
            "type": "TwoPieceTemplate",
            "properties": {
              "orientation": "vertical",
              "valign": "center"
            },
            "slots": {
              "first": {
                "type": "AppBanner",
                "properties": {
                  "margin-bottom": 42
                }
              },
              "second": {
                "type": "SearchBox",
                "properties": {
                  "halign": "center"
                }
              }
            }
          },
          "lightbox": {
            "type": "MediaLightbox",
            "slots": {
              "card": {
                "type": "MediaCard"
              }
            }
          },
          "search-page": {
            "type": "DividedBannerTemplate",
            "slots": {
              "bottom": {
                "type": "PaperTemplate",
                "slots": {
                  "content": {
                    "type": "BannerTemplate",
                    "properties": {
                      "margin-end": 45,
                      "margin-start": 45
                    },
                    "slots": {
                      "banner": {
                        "type": "SearchBanner",
                        "properties": {
                          "halign": "start"
                        }
                      },
                      "content": {
                        "type": "Search",
                        "properties": {
                          "halign": "fill",
                          "margin-top": 20,
                          "message-halign": "start"
                        },
                        "slots": {
                          "arrangement": {
                            "type": "ScrollingListArrangement",
                            "properties": {
                              "halign": "fill",
                              "hexpand": true
                            },
                            "slots": {
                              "card": {
                                "type": "TitleCard"
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              },
              "top-left": {
                "type": "AppBanner",
                "properties": {
                  "halign": "center",
                  "margin-bottom": 10,
                  "margin-top": 10,
                  "valign": "start",
                  "vexpand": false
                }
              },
              "top-right": {
                "type": "SearchBox",
                "properties": {
                  "halign": "center",
                  "hexpand": true,
                  "valign": "center"
                }
              }
            }
          }
        }
      }
    }
  }
}
